<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.urldizhiMapper" >

    <delete id="delteUrlid">
    delete from urldizhi where urlid=#{urlid}
</delete>

    <insert id="addUrdizhi">
    insert urldizhi values(default ,#{current},#{urlBeizhu},#{m3u8} ,#{xiazaiBeizhu},#{mp4})
</insert>



</mapper>